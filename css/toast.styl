.toast-container
	position: fixed
	width: 300px
	top: 20px
	right: 20px
	z-index: 9999

	.toast
		position: relative
		opacity: 1
		box-shadow: none
		font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif
		font-size: 15px
		line-height: 24px
		text-align: left
		width: 100%
		box-sizing: border-box
		-webkit-animation: 0.5s toast_appear cubic-bezier(0,.73,.26,.97)
		-moz-animation: 0.5s toast_appear cubic-bezier(0,.73,.26,.97)
		-o-animation: 0.5s toast_appear cubic-bezier(0,.73,.26,.97)
		animation: 0.5s toast_appear cubic-bezier(0,.73,.26,.97)
		margin-top: 0
		margin-bottom: 10px
		box-shadow: 0 0 7px #000
		cursor: pointer
		background-color: rgba(0,0,0,0.9)
		color: #fff
		border-radius: 8px
		border: 1px solid #000

		span
			border-radius: 8px;
			box-sizing: border-box;
			padding: 15px 20px 15px 50px;
			border: solid 1px #444
			width: 100%
			height: 100%
			display: block
			background-position: 15px center;
			background-repeat: no-repeat;

